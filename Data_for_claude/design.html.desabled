<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Anuc Home Benefits Section</title>
    <style>
      * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
      }

      :root {
        --header-height: 5rem;
        --columns: 4;
      }

      body {
        font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto,
          sans-serif;
        background: white;
      }

      /* Dummy Sections */
      .dummy-section {
        height: 100vh;
        display: flex;
        align-items: center;
        justify-content: center;
      }

      .dummy-above {
        background: linear-gradient(135deg, #fef3c7 0%, #e7e5e4 100%);
      }

      .dummy-below {
        background: linear-gradient(135deg, #e7e5e4 0%, #fef3c7 100%);
      }

      .dummy-title {
        font-size: 6rem;
        font-family: Georgia, serif;
        margin-bottom: 1rem;
      }

      .dummy-text {
        font-size: 1.25rem;
        color: #4b5563;
      }

      /* Main Section */
      .overflow-x-clip {
        overflow-x: clip;
      }

      .px-8 {
        padding-left: 2rem;
        padding-right: 2rem;
      }

      .relative {
        position: relative;
      }

      .isolate {
        isolation: isolate;
      }

      .size-full {
        width: 100%;
        height: 100%;
      }

      .border-x {
        border-left: 1px solid rgba(212, 212, 216, 0.2);
        border-right: 1px solid rgba(212, 212, 216, 0.2);
      }

      /* Grid Lines */
      .pointer-events-none {
        pointer-events: none;
      }

      .absolute {
        position: absolute;
      }

      .inset-0 {
        inset: 0;
      }

      .translate-x-sm {
        transform: translateX(0.15rem);
      }

      .grid {
        display: grid;
      }

      .grid-cols-2 {
        grid-template-columns: repeat(2, 1fr);
      }

      .grid-cols-4 {
        grid-template-columns: repeat(4, 1fr);
      }

      .gap-4 {
        gap: 1rem;
      }

      .h-full {
        height: 100%;
      }

      .hidden {
        display: none;
      }

      .origin-top {
        transform-origin: top;
      }

      .grid-line::after {
        content: "";
        position: absolute;
        inset: 0;
        right: 0;
        transform: translateX(100%);
        background: white;
        width: 1px;
      }

      /* Background */
      .bg-base-stone-20 {
        background-color: #f5f5f4;
      }

      /* Cards Grid */
      .w-full {
        width: 100%;
      }

      .-mt-1 {
        margin-top: -1px;
      }

      .first-mt-0:first-child {
        margin-top: 0;
      }

      .sticky {
        position: sticky;
      }

      .top-header {
        top: var(--header-height);
      }

      .flex {
        display: flex;
      }

      .aspect-square {
        aspect-ratio: 1;
      }

      .aspect-0-7 {
        aspect-ratio: 0.7;
      }

      .flex-col {
        flex-direction: column;
      }

      .justify-between {
        justify-content: space-between;
      }

      .gap-y-24 {
        row-gap: 6rem;
      }

      .border-y {
        border-top: 1px solid white;
        border-bottom: 1px solid white;
      }

      .p-8 {
        padding: 2rem;
      }

      .block {
        display: block;
      }

      .whitespace-pre-line {
        white-space: pre-line;
      }

      .text-balance {
        text-wrap: balance;
      }

      .text-base {
        font-size: 1.125rem;
        color: #374151;
        line-height: 1.75;
      }

      .font-serif {
        font-family: Georgia, serif;
      }

      .text-title-20 {
        font-size: 1.5rem;
        line-height: 1.2;
      }

      .col-start-2 {
        grid-column-start: 2;
      }

      /* CTA */
      .py-96 {
        padding-top: 24rem;
        padding-bottom: 24rem;
      }

      .cta-button {
        position: relative;
        display: inline-flex;
        align-items: center;
        justify-content: center;
        gap: 0.5rem;
        cursor: pointer;
        user-select: none;
        text-transform: uppercase;
        height: 2.25rem;
        border-top: 1px solid white;
        padding: 0 0.5rem;
        color: black;
        font-size: 0.75rem;
        letter-spacing: 0.05em;
        background: transparent;
        border-left: none;
        border-right: none;
        border-bottom: none;
        font-family: inherit;
      }

      .cta-bg {
        pointer-events: none;
        position: absolute;
        inset: 0;
        display: block;
        transition: transform 1s ease-out;
        transform-origin: left;
        transform: scaleX(0);
        background: #e7e5e4;
        z-index: -1;
      }

      .cta-button:hover .cta-bg {
        transform: scaleX(1);
      }

      .cta-icon-wrapper {
        display: flex;
        width: 1.25rem;
        height: 1.25rem;
        flex-shrink: 0;
        align-items: center;
        overflow: hidden;
        border-radius: 50%;
        transition: all 1s ease-out;
        background: #d6d3d1;
        color: black;
      }

      .cta-button:hover .cta-icon-wrapper {
        border-radius: 0;
        background: #a8a29e;
      }

      .cta-icon {
        display: flex;
        width: 1.25rem;
        height: 1.25rem;
        flex-shrink: 0;
        align-items: center;
        justify-content: center;
        transition: transform 1s ease-out;
        transform: translateX(-100%);
      }

      .cta-button:hover .cta-icon {
        transform: translateX(0);
      }

      .icon-svg {
        height: auto;
        width: 100%;
      }

      /* Responsive */
      @media (min-width: 1024px) {
        .lg-hidden {
          display: none !important;
        }

        .lg-grid {
          display: grid !important;
        }

        .lg-block {
          display: block !important;
        }

        .lg-aspect-square {
          aspect-ratio: 1;
        }

        .lg-p-24 {
          padding: 6rem;
        }

        .lg-border-x-0 {
          border-left: none;
          border-right: none;
        }

        .lg-col-start-2 {
          grid-column-start: 2;
        }

        .text-title-20 {
          font-size: 3rem;
        }

        .dummy-title {
          font-size: 6rem;
        }
      }

      @media (max-width: 1023px) {
        .dummy-title {
          font-size: 4rem;
        }
      }
    </style>
  </head>
