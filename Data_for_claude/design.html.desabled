                          <div class="booking-slider-item__amenities">
                            <div class="booking-slider-item__amenity">
                              <div
                                class="booking-slider-item__amenity-icon"
                              >
                                <img
                                  src="https://bato-web-agency.github.io/bato-shared/img/slider-2/icon-beds.svg"
                                  alt="Beds"
                                />
                              </div>
                              <div class="booking-slider-item__amenity-text">
                                5 Beds
                              </div>
                            </div>

                            <div class="booking-slider-item__amenity">
                              <div
                                class="booking-slider-item__amenity-icon"
                              >
                                <img
                                  src="https://bato-web-agency.github.io/bato-shared/img/slider-2/icon-bathrooms.svg"
